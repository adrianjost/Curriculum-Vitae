{
	"name": "adrianjost-cv",
	"version": "3.1.0",
	"description": "Personal Homepage of Adrian Jost. You can find most of his project work and his contact information. If you are impressed you could even offer him a nice Job. ;) He would be very happy to hear from you!",
	"author": "Adrian Jost",
	"license": "MIT",
	"scripts": {
		"dev:nuxt": "cross-env NODE_ENV=development nuxt",
		"dev:functions": "cd functions && npm run serve",
		"dev": "cross-env USE_LOCAL_API=true run-p dev:*",
		"analyze": "nuxt build --analyze",
		"build": "cross-env NODE_ENV=production nuxt generate",
		"deploy": "npm run lint && npm run build && firebase deploy",
		"lint:eslint": "eslint --fix",
		"lint:stylelint": "stylelint --fix",
		"lint:prettier": "prettier --write --loglevel warn",
		"lint:all:eslint": "npm run lint:eslint -- --ext .js,.vue .",
		"lint:all:stylelint": "npm run lint:stylelint \"src/**/*.{vue,sass,scss,css}\"",
		"lint:all:prettier": "npm run lint:prettier \"**/*.{js,json,css,scss,vue,html}\"",
		"lint": "run-s lint:all:*"
	},
	"gitHooks": {
		"pre-commit": "cross-env PRE_COMMIT=true lint-staged"
	},
	"dependencies": {
		"@johmun/vue-tags-input": "^2.1.0",
		"autosize": "^4.0.2",
		"base64-img": "^1.0.4",
		"firebase": "^7.8.0",
		"firebaseui": "^4.4.0",
		"isomorphic-unfetch": "^3.0.0",
		"nuxt": "^2.12.2",
		"v-lazy-image": "^1.4.0",
		"vue-pose": "^0.5.1",
		"vuewordcloud": "^18.7.11"
	},
	"devDependencies": {
		"@nuxtjs/pwa": "^3.0.0-beta.8",
		"@nuxtjs/sitemap": "^2.0.1",
		"babel-eslint": "^10.1.0",
		"core-js": "^3.6.5",
		"cross-env": "^7.0.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-vue": "^6.2.2",
		"imagemin-lint-staged": "^0.4.0",
		"lint-staged": "^10.0.7",
		"node-sass": "^4.14.0",
		"npm-run-all": "^4.1.5",
		"prettier": "github:prettier/prettier#3de36e3",
		"sass-loader": "^8.0.2",
		"stylelint": "^13.3.3",
		"stylelint-config-prettier": "^8.0.1",
		"stylelint-config-recess-order": "^2.0.4",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-scss": "^3.14.2",
		"vue": "^2.6.11",
		"yorkie": "^2.0.0"
	},
	"engines": {
		"node": "10"
	}
}
